window.onscroll=function(){upbtn=document.querySelector("#up-btn");upbtn.style.display=50<document.body.scrollTop||50<document.documentElement.scrollTop?"block":"none"};function topFunction(){document.body.scrollTop=0;document.documentElement.scrollTop=0}function modalShow(a,b){document.querySelector("#base-modal-label").innerHTML=a;document.querySelector("#base-modal-body").innerHTML=b;jQuery("#base-modal").modal()};
